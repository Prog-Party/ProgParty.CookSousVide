@{
    ViewData["Title"] = "Cook Sous Vide";
}


<!--main-->
<section class="main">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-sm-6">
                <!--logo-->
                <div class="logo"><img src="img/logo.png" data-at2x="img/logo@2x.png" alt="logo"></div>
                <!--logo end-->
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">

                <!--welcome-message-->
                <header class="welcome-message text-center">
                    <h1><span class="rotate">Cook sous vide</span></h1>
                </header>
                <!--welcome-message end-->
                <!--sub-form-->
                <div class="sub-form text-center">
                    <div class="row">
                        <div class="col-md-5 center-block col-sm-8 col-xs-11">
                            <form role="form" id="mc-form" action="http://csmthemes.us3.list-manage.com/subscribe/post-json?u=9666c25a337f497687875a388&amp;id=5b881a50fb&amp;c=?">
                                <div class="input-group">
                                    <input type="search" class="form-control" placeholder="Sous vide product" required value="" name="SousVideProduct">
                                    <span class="input-group-btn">
                                        <button type="submit" class="btn btn-default" id="mc-subscribe" value="SousVideProduct" name="sousVideProduct">Search<i class="fas fa-search"></i></button>
                                    </span>
                                </div>
                                <div class="input-group">
                                    <input type="search" class="form-control" placeholder="Meat type" required value="" name="MeatType">
                                    <span class="input-group-btn">
                                        <button type="submit" class="btn btn-default" id="mc-subscribe" value="MeatType" name="meatType">Search<i class="fas fa-search"></i></button>
                                    </span>
                                </div>
                            </form>
                            <p id="mc-notification"></p>
                        </div>
                    </div>
                </div>
                <!--sub-form end-->
            </div>
        </div>
    </div>
</section>
<!--main end-->
<!--Features-->

<section class="features section-spacing">
    <div class="container">
        <h2 class="text-center">Featered services</h2>
        <div class="row">
            <div class="col-md-6">
                <div class="wow fadeInUp product-features row">
                    <div class="col-md-2 col-sm-2 col-xs-2 text-center"><i class="fa fa-thermometer-quarter fa-3x"></i></div>
                    <div class="col-md-10 col-sm-10 col-xs-10">
                        <h4>Temperature of product</h4>

                        <div class="row">
                            <div class="col-md-4" style="text-align:right;">
                                Rare
                            </div>
                            <div class="col-md-4">
                            </div>
                            <div class="col-md-4">
                                Well done
                            </div>
                        </div>
                        <div class="progress" style="margin-bottom:0px; height:30px;">
                            <div class="progress-bar progress-bar-danger" role="progressbar" style="width:30%">
                            </div>
                            <div class="progress-bar progress-bar-success" role="progressbar" style="width:40%">
                            </div>
                            <div class="progress-bar progress-bar-danger" role="progressbar" style="width:30%">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4" style="text-align:right;">
                                52.0&deg;
                            </div>
                            <div class="col-md-4">

                            </div>
                            <div class="col-md-4">
                                56.0&deg;
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!--Features end-->